{
  "name": "hook.io-daily-epf-import",
  "description" : "A hook that performs the daily import from the Apple iTunes EPF server.",
  "subdomain": "hook.io-daily-epf-import",
  "homepage": "http://freshfugu.com",
  "author": "Martin Wawrusch (http://twitter.com/martin_sunset)",
  "directories": {
    "lib": "./lib",
    "test": "./spec"
  },
  "bin" : {
    "daily-epf-import" : "./bin/daily-epf-import"
  },
  "keywords" : [
    "epf","hook.io","apple","itunes"
  ],
  
  "scripts": {
    "start": "bin/daily-epf-import",
    "test": "vows --spec spec/*-spec.coffee",
    "watch" : "coffee -c -w  -o lib src"
  },
  "engines": {
      "node": ">= 0.4.08"
    },
  "dependencies": {
    "hook.io" : ">= 0.7.7",
    "tar-async" : ">= 1.1.1",
    "hook.io-cron" : ">= 0.4.0",
    "underscore": ">= 1.1.7",
    "mongoose": ">= 2.2.3",
    "async": ">= 0.1.6",
    "colors" : ">= 0.5.0",
    "should" : ">=0.2.1",
    "vows" : ">= 0.5.9",
    "nconf" : ">= 0.4.3",
    "itunes-epf-feedcheck" : ">= 0.0.1",
    "persistent-task-status" : ">= 0.0.1",
    "hook.io-wget": ">= 0.0.4"
    
  },
  "version": "0.0.1"

}
